for i in range (int(input())) :
    a = list(input().split())
    a1 = float(a[0])
    
    for j in range (len(a)) :
        if a[0+j] == '@' :
            a1 = a1*3
        if a[0+j] == '%' :
            a1 = a1+5
        if a[0+j] == '#' :
            a1 = a1-7
    
    print("{0:0.2f}".format(a1))
