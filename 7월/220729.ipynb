   "source": [
    "n = input().upper()\n",
    "nset = list(set(n)) #중복값 제거\n",
    "cnt = []\n",
    "\n",
    "for i in nset :\n",
    "    cnt.append(n.count(i)) # 입력된 문자 요소 갯수 세는 리스트 추가\n",
    "\n",
    "if cnt.count(max(cnt)) > 1 :\n",
    "    print(\"?\") # 최댓값이 2개 이상이면 ?\n",
    "else :\n",
    "    print(nset[cnt.index(max(cnt))]) # 가장 많은 요소 출력"
   ]
  }
