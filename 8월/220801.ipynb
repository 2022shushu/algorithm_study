    "n = input()\n",
    "dial = ['ABC', 'DEF', 'GHI', 'JKL', 'MNO', 'PQRS', 'TUV', 'WXYZ']\n",
    "cnt = 0\n",
    "\n",
    "for i in range(len(n)) :\n",
    "    for j in dial :\n",
    "        if n[i] in j :\n",
    "            cnt = cnt + dial.index(j) + 3\n",
    "\n",
    "print(cnt)"
